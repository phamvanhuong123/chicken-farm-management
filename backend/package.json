    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "jsdom": "^27.3.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "vitest": "^4.0.15"
  },
